class Solution:
    def targetIndices(self, nums: List[int], target: int) -> List[int]:
        # nums = [1,2,5,2,3]
        # target = 2
        nums.sort()
        # print(nums)
        s = []
        # print(nums)
        for i in range(len(nums)):
            if nums[i] == target:
                s.append(i)
        return(s)
